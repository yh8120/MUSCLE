<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.dao.ProteinDao">
	
	<insert id="insert" parameterType="com.example.app.domain.RestProtein">
		INSERT INTO proteins
		(user_id, training_log_id,regestred)
		VALUES (#{uid},#{tid},NOW())
	</insert>
	<delete id="delete" parameterType="com.example.app.domain.RestProtein">
	DELETE FROM proteins
		WHERE user_id=#{uid} 
		AND training_log_id=#{tid}
	</delete>

</mapper>
